{
  "name": "express-inversify-template",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc",
    "start": "npm-run-all build start:prod",
    "dev": "nodemon --watch src -e ts,ejs,json,js --exec npm run start",
    "start:prod": "node .",
    "prepare": "husky install",
    "prettier": "prettier --ignore-path .gitignore --ignore-path .prettierignore --write \"src/**/*.ts\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "husky": "^8.0.0",
    "lint-staged": "^15.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@types/jest": "^29.5.11",
    "dotenv": "^16.0.1",
    "jest": "^29.7.0",
    "module-alias": "^2.2.3",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.1"
  },
  "_moduleAliases": {
    "@config": "dist/config",
    "@services": "dist/services",
    "@models": "dist/models",
    "@controllers": "dist/controllers",
    "@middlewares": "dist/middlewares",
    "@schemas": "dist/schemas",
    "@enums": "dist/enums",
    "@errors": "dist/errors",
    "@utils": "dist/utils"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run prettier"
    ]
  }
}
